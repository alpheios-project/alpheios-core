{
  "name": "alpheios-core",
  "version": "1.0.1-dev.0",
  "private": false,
  "author": "The Alpheios Project, Ltd.",
  "license": "ISC",
  "scripts": {
    "set-node-build-deps": "npm i -D git://github.com/alpheios-project/node-build.git#v2 && npx install-peerdeps alpheios-node-build --dev --only-peers",
    "bootstrap": "npx lerna bootstrap --hoist --nohoist=alpheios-node-build --nohoist=eslint-config-standard --nohoist=coveralls",
    "clean": "npx lerna clean --yes",
    "list-mismatching-deps": "npx syncpack list-mismatches; exit(0)",
    "list-outdated-deps": "npx lerna exec --no-bail npm outdated",
    "fix-mismatching-deps": "npx syncpack fix-mismatches",
    "build": "npm run build-data-models && npm run build-res-cleint && npm run build-wordlist && npm run build-client-adapters && npm run build-inflection-tables && npm run build-components",
    "test": "npm run test-data-models; npm run test-res-cleint; npm run test-wordlist; npm run test-client-adapters; npm run test-inflection-tables; npm run test-components",
    "test-ci": "npm --prefix packages/data-models run test-no-coverage && npm --prefix packages/res-client run test-no-coverage && npm --prefix packages/wordlist run test-no-coverage && npm --prefix packages/client-adapters run test-no-coverage && npm --prefix packages/inflection-tables run test-no-coverage && npm --prefix packages/components run test-no-coverage",
    "build-data-models": "npm --prefix packages/data-models run build",
    "test-data-models": "npm --prefix packages/data-models run test",
    "build-res-cleint": "npm --prefix packages/res-client run build",
    "test-res-client": "npm --prefix packages/res-client run test",
    "build-wordlist": "npm --prefix packages/wordlist run build",
    "test-wordlist": "npm --prefix packages/wordlist run test",
    "build-client-adapters": "npm --prefix packages/client-adapters run build",
    "test-client-adapters": "npm --prefix packages/client-adapters run test",
    "build-inflection-tables": "npm --prefix packages/inflection-tables run build",
    "test-inflection-tables": "npm --prefix packages/inflection-tables run test",
    "build-components": "npm --prefix packages/components run build",
    "test-components": "npm --prefix packages/components run test-ci",
    "conventional-commit": "npx git-cz",
    "version-set-major-dev": "npx lerna version major --no-push",
    "version-set-minor-dev": "npx lerna version minor --no-push",
    "version-set-patch-dev": "npx lerna version patch --no-push",
    "tagged-commit": "node ./tagged-commit.mjs"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-transform-modules-commonjs": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "alpheios-node-build": "git://github.com/alpheios-project/node-build.git#v2",
    "archiver": "^3.1.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "chalk": "^3.0.0",
    "command-line-args": "^5.1.1",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "fibers": "^4.0.2",
    "fs-extra": "^8.1.0",
    "git-branch": "^2.0.1",
    "imagemin": "^7.0.1",
    "imagemin-jpegtran": "^6.0.0",
    "imagemin-optipng": "^7.1.0",
    "imagemin-svgo": "^7.0.0",
    "inspectpack": "^4.3.1",
    "lerna": "^3.20.2",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "path": "^0.12.7",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-safe-important": "^1.2.0",
    "postcss-scss": "^2.0.0",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "url-loader": "^3.0.0",
    "vue-loader": "^15.8.3",
    "vue-svg-loader": "^0.16.0",
    "vue-template-compiler": "^2.6.11",
    "vue-template-loader": "^1.0.0",
    "webpack": "^4.41.6",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {}
}
