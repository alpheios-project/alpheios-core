    "build-safari": "npm run lint && node --experimental-modules ../../node_modules/alpheios-node-build/dist/build.mjs -m all -M all -p vue-postcss -c config-safari.mjs",
    "build-prod": "npm run lint && node --experimental-modules ../../node_modules/alpheios-node-build/dist/build.mjs -m all -M production -p vue -c config.mjs",
    "build-dev": "npm run lint && node --experimental-modules ../../node_modules/alpheios-node-build/dist/build.mjs -m all -M development -p vue -c config.mjs",
    "code-analysis-prod": "node --experimental-modules ../../node_modules/alpheios-node-build/dist/build.mjs -m all -M production -p vue -c config.mjs -a",
    "code-analysis-dev": "node --experimental-modules ../../node_modules/alpheios-node-build/dist/build.mjs -m all -M development -p vue -c config.mjs -a",
    "lint": "npx eslint --fix src/**/*.js",
    "lint-stansard": "npx eslint --no-eslintrc -c eslint-standard-conf.json --fix src/**/*.js",
    "lint-jsdoc": "npx eslint --no-eslintrc -c eslint-jsdoc-conf.json src/**/*.js",
    "lint-vue": "npx eslint --no-eslintrc --fix-dry-run -c eslint-vue-conf.json src/**/*.vue"
