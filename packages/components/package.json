    "vue-eslint-parser": "^6.0.4",
    "vue-jest": "^3.0.5",
    "vue-loader": "^15.7.1",
    "vue-multiselect": "^2.1.6",
    "vue-style-loader": "^4.1.2",
    "vue-svg-loader": "^0.12.0",
    "vue-template-compiler": "^2.6.10",
