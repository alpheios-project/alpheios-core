{
  "name": "alpheios-components",
  "version": "0.4.6",
  "description": "Alpheios UI Compononents",
  "main": "dist/alpheios-components.min.js",
  "scripts": {
    "test": "standard --fix && jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test-lib": "standard --fix && jest tests/lib --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test-components": "standard --fix && jest tests/vue-components --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test-c": "standard --fix && jest tests/vue-components/lookup.test.js --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "test-a": "standard --fix && jest tests/lib/selection/alignment --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "build": "standard --fix && node --experimental-modules ./node_modules/alpheios-node-build/dist/build.mjs all all vue",
    "build-prod": "standard --fix && node --experimental-modules ./node_modules/alpheios-node-build/dist/build.mjs all production vue",
    "build-dev": "standard --fix && node --experimental-modules ./node_modules/alpheios-node-build/dist/build.mjs all development vue",
    "lint": "standard --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alpheios-project/components.git"
  },
  "author": "The Alpheios Project, Ltd.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alpheios-project/components/issues"
  },
  "homepage": "https://github.com/alpheios-project/components#readme",
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.25",
    "acorn": "^6.0.2",
    "alpheios-data-models": "github:alpheios-project/data-models",
    "alpheios-experience": "github:alpheios-project/experience",
    "alpheios-inflection-tables": "github:alpheios-project/inflection-tables",
    "alpheios-lemma-client": "github:alpheios-project/lemma-client",
    "alpheios-lexicon-client": "github:alpheios-project/lexicon-client",
    "alpheios-morph-client": "github:alpheios-project/morph-client",
    "alpheios-node-build": "github:alpheios-project/node-build",
    "alpheios-res-client": "github:alpheios-project/res-client",
    "autoprefixer": "^9.1.5",
    "bytes": "^3.0.0",
    "chalk": "^2.4.1",
    "coveralls": "^3.0.2",
    "css-loader": "^1.0.0",
    "dom-anchor-text-quote": "*",
    "element-closest": "^2.0.2",
    "espree": "^4.0.0",
    "file-loader": "^2.0.0",
    "flush-promises": "^1.0.0",
    "font-awesome-svg-png": "^1.2.2",
    "html-loader": "^0.5.5",
    "html-loader-jest": "^0.2.1",
    "interactjs": "^1.3.3",
    "intl-messageformat": "^2.2.0",
    "jest": "^23.6.0",
    "jest-serializer-vue": "^2.0.2",
    "jest-vue-preprocessor": "^1.4.0",
    "jump.js": "^1.0.2",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^7.1.0",
    "shelljs": "^0.8.2",
    "sinon": "^6.3.4",
    "source-map-loader": "^0.2.4",
    "standard": "^12.0.1",
    "style-loader": "^0.23.0",
    "uikit": "^3.0.0-rc.17",
    "vue": "^2.5.17",
    "whatwg-fetch": "^3.0.0",
    "wrap-range-text": "*"
  },
  "peerDependencies": {
    "alpheios-data-models": "github:alpheios-project/data-models",
    "alpheios-experience": "github:alpheios-project/experience",
    "alpheios-inflection-tables": "github:alpheios-project/inflection-tables",
    "alpheios-res-client": "github:alpheios-project/res-client",
    "dom-anchor-text-quote": "4.0.2",
    "intl-messageformat": "2.x.x",
    "uuid": "3.x.x",
    "wrap-range-text": "",
    "eslint": "^1.6.0"
  },
  "engines": {
    "node": ">= 10.5.0",
    "npm": ">= 5.6.0"
  },
  "jest": {
    "verbose": true,
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "^.+\\.htmlf$": "html-loader-jest",
      "^.+\\.jsx?$": "babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor",
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "!node_modules/alpheios-data-models/"
    ],
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.common.js",
      "^@[/](.+)": "<rootDir>/src/$1",
      "alpheios-morph-client": "<rootDir>/node_modules/alpheios-morph-client/dist/alpheios-morph-client.js",
      "alpheios-inflection-tables": "<rootDir>/node_modules/alpheios-inflection-tables/dist/alpheios-inflection-tables.js"
    },
    "moduleFileExtensions": [
      "js",
      "vue"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module",
      "experimentalObjectRestSpread": true
    }
  },
  "standard": {
    "ignore": [
      "**/dist",
      "**/support"
    ]
  },
  "dependencies": {
    "vue-multiselect": "^2.1.0"
  }
}
