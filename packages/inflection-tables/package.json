{
  "name": "alpheios-inflection-tables",
  "version": "0.1.0",
  "description": "Inflection library for an Alpheios project",
  "main": "dist/inflection-tables.js",
  "directories": {
    "doc": "doc"
  },
  "scripts": {
    "test": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "build": "node ./support/rollup/build.js",
    "buildextensionbackground": "node build-extension-background.js",
    "buildextensioncontent": "node build-extension-content.js",
    "buildwebext": "webpack --config webextension/support/webpack/webpack.config.js",
    "convert": "node ./tools/inflections-xml2csv/converter.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alpheios-project/inflection-tables.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alpheios-project/inflection-tables/issues"
  },
  "homepage": "https://github.com/alpheios-project/inflection-tables#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babyparse": "^0.4.6",
    "font-awesome-svg-png": "^1.2.2",
    "intl-messageformat": "^2.2.0",
    "jest": "^21.2.1",
    "jest-environment-jsdom-11.0.0": "^20.0.9",
    "papaparse": "^4.3.6",
    "raw-loader": "^0.5.1",
    "rollup": "^0.47.6",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-string": "^2.0.2",
    "the-answer": "^1.0.0",
    "uuid": "^3.1.0",
    "vue": "^2.5.3",
    "webextension-polyfill": "^0.2.1",
    "webpack": "^3.8.1",
    "xmldom": "^0.1.27",
    "xmltojson": "^1.3.5"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "jest-environment-jsdom-11.0.0"
  },
  "dependencies": {
    "alpheios-data-models": "git://github.com/alpheios-project/data-models.git",
    "alpheios-tufts-adapter": "git://github.com/alpheios-project/tufts-adapter.git",
    "coveralls": "^3.0.0"
  }
}
