<div class="infl-cell infl-cell--sp1"
     data-role="suffix-cell"
     data-empty="{{empty}}"
     data-suffix-matches="{{suffixMatches}}"
     data-row-values="{{rowValues}}"
     data-column-values="{{columnValues}}">
{{#each suffixes}}
    <a class="infl-suff{{#if match.suffixMatch}} infl-suff--suffix-match{{/if}}{{#if match.fullMatch}} infl-suff--full-feature-match{{/if}}">{{#if value}}{{value}}{{else}}-{{/if}}&nbsp;{{#if footnote.length}}[{{footnote}}]{{/if}}</a>{{#unless @last}}&nbsp;,&nbsp;{{/unless}}
{{/each}}
</div>