{
  "manifest_version": 2,
  "name": "WebExtension Test",
  "version": "1.0",
  "description": "Adds a test window to specified pages",
  "icons": {
    "48": "icons/ic_device_hub_black_24px.svg"
  },

  "background": {
    "scripts": [
      "webextension-polyfill-0.1.2-fixed/browser-polyfill.js",
      "background.js"]
  },

  "page_action": {
    "default_icon": "icons/off.svg",
    "browser_style": true
  },

  "permissions": [
    "activeTab",
    "tabs"
  ]
}